#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/starlight/node_modules/parse-domain/bin/node_modules:/workspaces/starlight/node_modules/parse-domain/node_modules:/workspaces/starlight/node_modules:/workspaces/node_modules:/node_modules:/workspaces/starlight/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/starlight/node_modules/parse-domain/bin/node_modules:/workspaces/starlight/node_modules/parse-domain/node_modules:/workspaces/starlight/node_modules:/workspaces/node_modules:/node_modules:/workspaces/starlight/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../parse-domain/bin/update.js" "$@"
else
  exec node  "$basedir/../parse-domain/bin/update.js" "$@"
fi
